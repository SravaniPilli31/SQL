`Customer Address` VARCHAR(45) NULL,
`City` VARCHAR(45) NULL,
`State` VARCHAR(45) NULL,
`Zip` INT NULL,
`Phone Number` INT NULL,
`Extension` INT NULL,
`Fax Number` VARCHAR(45) NULL,
`Email Address` VARCHAR(45) NULL,
`Sub Contractor ID` VARCHAR(45) NULL,
`Vendor Company` VARCHAR(45) NULL,
