`Supervisor Phone` INT NULL,
`Supervisor eMail` VARCHAR(45) NULL,
`Priority` VARCHAR(45) NULL,
`Payment Status` VARCHAR(45) NULL,
`Project Notes` VARCHAR(45) NULL,
`Invoice Notes` VARCHAR(45) NULL,
`Client_ID` VARCHAR(45) NOT NULL,
PRIMARY KEY (`Employee ID`),
