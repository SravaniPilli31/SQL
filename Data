CLIENT Table:

CREATE TABLE `Client` (
  `Client_ID` INT NOT NULL,
  `Client_Name` VARCHAR(45) NOT NULL,
  `Client_DBA` VARCHAR(45) NOT NULL,
  `Client_Address` VARCHAR(45) NOT NULL,
  `Client_City` VARCHAR(45) NOT NULL,
  `Client_State` VARCHAR(45) NOT NULL,
  `Client_Zip` VARCHAR(45) NOT NULL,
  `Client_EIN` VARCHAR(45) NOT NULL,
  `Client_Phone` VARCHAR(45) NOT NULL,
  `Client_Fax` VARCHAR(45) NOT NULL,
  `Client_Email` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`Client_ID`));


Employee Table:

CREATE TABLE `actaxdev`.`Employee` (
`Employee_ID` VARCHAR(20) NOT NULL,
`Flag` VARCHAR(45) NOT NULL,
`Last Name` VARCHAR(45) NOT NULL,
`First Name` VARCHAR(45) NOT NULL,
`Middle Name` VARCHAR(45) NULL,
`Employee Type` VARCHAR(45) NOT NULL,
`Photograph` BLOB NOT NULL,
`TimeStamp` DATETIME NOT NULL,
`H1BForm Submitted` VARCHAR(45) NOT NULL,
`Employee Status` VARCHAR(45) NOT NULL,
`Refered By` VARCHAR(45) NOT NULL,
`Department` VARCHAR(45) NOT NULL,
`Business Unit` VARCHAR(45) NOT NULL,
`Reports To` VARCHAR(45) NOT NULL,
`Date of Hiring` DATETIME NOT NULL,
`Date of Joining` DATETIME NOT NULL,
`Date of Leaving` DATETIME NOT NULL,
`Reason for  In-Active` VARCHAR(45) NOT NULL,
`Social Security Number` INT NOT NULL,
`Gender` VARCHAR(7) NOT NULL,
`Birthdate` DATETIME NOT NULL,
`Drivers License  .` INT NOT NULL,
`Drivers License State` VARCHAR(45) NOT NULL,
`Marital Status` VARCHAR(45) NOT NULL,
`Spouse Name` VARCHAR(45) NULL,
`Spouse Birthdate` DATETIME NULL,
`Child1 Name` VARCHAR(45) NULL,
`Child1 DOB` DATETIME NULL,
`Child2 Name` VARCHAR(45) NULL,
`Child2 DOB` DATETIME NULL,
`Current Address` VARCHAR(45) NOT NULL,
`Address2` VARCHAR(45) NULL,
`Current City` VARCHAR(45) NOT NULL,
`Current State` VARCHAR(45) NOT NULL,
`Current Zip Code` INT NOT NULL,
`Current Country` VARCHAR(45) NOT NULL,
`EMail` VARCHAR(45) NOT NULL,
`Official Email ID` VARCHAR(45) NOT NULL,
`ForeignCell` VARCHAR(45) NULL,
`Home Phone` INT NULL,
`Work Phone` VARCHAR(45) NULL,
`Ext` VARCHAR(45) NULL,
`Fax Number` VARCHAR(45) NULL,
`Mobile Phone` VARCHAR(45) NOT NULL,
`US Reference Name` VARCHAR(45) NULL,
`US Reference Phone` INT NULL,
`Foreign Address` VARCHAR(45) NULL,
`Foreign City` VARCHAR(45) NULL,
`Taluq` VARCHAR(45) NULL,
`District` VARCHAR(45) NULL,
`Foreign State` VARCHAR(45) NULL,
`Foreign Zip` INT NULL,
`Foreign Country` VARCHAR(45) NULL,
`Emrgcy Contact Name` VARCHAR(45) NULL,
`Emrgcy Contact Phone` INT NULL,
`Skills` VARCHAR(45) NULL,
`Job Duties` VARCHAR(45) NULL,
`Personal  Notes` VARCHAR(45) NULL,
`HR  Notes` VARCHAR(45) NULL,
`References` VARCHAR(45) NULL,
`Client_ID` VARCHAR(45) NULL,
PRIMARY KEY (`Employee_ID`),
CONSTRAINT `Client_ID`
FOREIGN KEY (Client_ID)
REFERENCES `actaxdev`.`Client` (Client_ID)
ON DELETE NO ACTION
ON UPDATE NO ACTION)

